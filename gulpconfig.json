{
	"styles": {
		"libs": [],
		"src": ["./source/assets/styles/main.scss"],
		"dest": "/assets/"
	},
	"scripts": {
		"libs": [
			"./source/libs/angular-route.js",
			"./source/libs/underscore.js"
		],
		"src": ["./source/app/**/*.js"],
		"dest": "/assets/"
	},
	"templates": {
		"copy": {
			"src": ["./source/app/views/*.html"],
			"dest": "/views"
		},
		"compile": {
			"src": ["./source/app/views/partials/*.html"],
			"dest": "/assets"
		}
	},
	"copy": {
		"src": [
			"./source/{favicon.ico,*.html,*.js}",
			"./source/**/{images,videos,fonts}/**/*"
		],
		"dest": "/"
	},
	"config": {
		"debug": {
			"name": "configiguration",
			"constants": {
				"foo":"bar"
			},
			"deps": []
		}
	},
	"protractor": {},
	"karma": {
		"basePath": "",
		"frameworks": ["jasmine"],
		"files": [
			"./tests/libs/angular.js",
			"./tests/libs/angular-mocks.js",
			"./source/libs/**/*.js",
			"./source/app/app.js",
			"./source/app/**/*.js",
			"./tests/specs/**/*.js"
		],
		"exclude": [],
		"preprocessors": {},
		"reporters": ["mocha"],
		"port": 9876,
		"colors": true,
		"autoWatch": false,
		"browsers": ["PhantomJS"],
		"singleRun": true
	}
}